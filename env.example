# Database Configuration
DATABASE_URL=postgresql://cms_user:cms_password@localhost:5432/cms_pricing
REDIS_URL=redis://localhost:6379/0

# API Configuration
API_KEYS=dev-key-123,admin-key-456,worker-key-789
RATE_LIMIT_PER_MINUTE=120

# Data Configuration
MAX_PROVIDERS=100
MAX_RADIUS_MILES=25
DEFAULT_FALLBACK_MULTIPLIER=1.0
RETENTION_MONTHS=13

# Storage Configuration
DATA_CACHE_DIR=./data/cache
S3_BUCKET=cms-pricing-data
S3_PREFIX=datasets
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1

# Logging Configuration
LOG_LEVEL=INFO
TRACE_VERBOSE=false
LOG_FORMAT=json

# Cache Configuration
CACHE_TTL_SECONDS=3600
CACHE_MAX_ITEMS=512
CACHE_MAX_BYTES=1073741824  # 1GB

# Security Configuration
SECRET_KEY=your-secret-key-here
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=60

# Performance Configuration
WARM_SLICES=MPFS:2025,OPPS:2025Q1,ASC:2025Q1
MAX_CONCURRENT_REQUESTS=25
BURST_LIMIT=100
